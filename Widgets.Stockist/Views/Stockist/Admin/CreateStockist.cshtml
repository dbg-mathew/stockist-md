@{
    Layout = "~/Administration/Views/Shared/_AdminLayout.cshtml";
}
@model Nop.Plugin.Widgets.DbgStockist.Models.StockistModel
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-configuration.png")" alt="" />
            @T("Admin.Common.AddNew")- <a href="/Plugins/Stockists">@T("Nop.Plugin.Widgets.DbgStockist.Stockist.Back")</a>
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>
    @Html.Partial("~/Plugins/Widgets.Stockist/Views/Stockist/Admin/_CreateOrUpdateStockist.cshtml", Model)
}

